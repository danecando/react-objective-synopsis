{
  "name": "react-workshop-starter",
  "version": "1.0.0",
  "description": "React workshop project boilerplate",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=development node server.js",
    "build": "NODE_ENV=production webpack --config ./webpack.config.production.js",
    "lint": "eslint src/*",
    "flow": "flow check",
    "test": "NODE_ENV=test mocha --require preprocessor.js --require babel-register src/**/*-test.js",
    "test:watch": "NODE_ENV=test mocha --require preprocessor.js --require babel-register src/**/*-test.js --watch --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danecando/react-workshop-starter.git"
  },
  "keywords": [
    "React",
    "Workshop",
    "Boilerplate",
    "UXDevSummit",
    "Redux"
  ],
  "author": "Dane Grant",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danecando/react-workshop-starter/issues"
  },
  "homepage": "https://github.com/danecando/react-workshop-starter#readme",
  "dependencies": {
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "spectacle": "^1.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.3.13",
    "browser-sync": "^2.11.0",
    "chai": "^3.4.1",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^2.1.0",
    "enzyme": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.14.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "markdown-loader": "^0.1.7",
    "mocha": "^2.3.4",
    "node-libs-browser": "^0.5.3",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.14",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.6",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
